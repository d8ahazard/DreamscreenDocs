<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <gradient android:startColor="#ffffffff" android:endColor="#ffffffff" android:angle="0"/>
            <corners android:radius="10dp"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip android:gravity="left" android:clipOrientation="horizontal">
            <shape>
                <gradient android:startColor="#f6d43b" android:endColor="#f6d43b" android:angle="0" android:centerX="0.75"/>
                <corners android:radius="10dp"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip android:gravity="left" android:clipOrientation="horizontal">
            <shape>
                <gradient android:startColor="#f6d43b" android:endColor="#f6d43b" android:angle="0"/>
                <corners android:radius="10dp"/>
            </shape>
        </clip>
    </item>
</layer-list>
